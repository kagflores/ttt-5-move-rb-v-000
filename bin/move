#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to Tic Tac Toe!"
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
board = get_variable_from_file("./bin/move", "board")
"Where would you like to go?"
index = gets.strip
input_to_index(index)
move (board, index, user_char)
display_board(board)
